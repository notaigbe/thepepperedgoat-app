{
  "expo": {
    "name": "Ooosum Foods LLC",
    "slug": "ooosum-foods-llc",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/jagabans-circle.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/jagabans-circle.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ooosumfoods.jagabansla",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/jagabans-circle.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ooosumfoods.jagabansla"
    },
    "web": {
      "favicon": "./assets/images/jagabans-circle.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "jagabansla",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "94e71267-25cb-411f-88e6-37bafccde801"
      }
    }
  },
  "scheme": "Ooosum Foods LLC"
}