{
  "expo": {
    "name": "Ooosum Foods LLC",
    "slug": "ooosum-foods-llc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/peppered-goat-logo.jpg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/peppered-goat-logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ooosumfoods.thepepperedgoat",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/peppered-goat-logo.jpg",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ooosumfoods.thepepperedgoat"
    },
    "web": {
      "favicon": "./assets/images/peppered-goat-logo.jpg",
      "bundler": "metro"
    },
    "plugins": [
      // [
      //   "expo-build-properties",
      //   {
      //     "ios": {
      //       "entitlements": {
      //         "com.apple.developer.in-app-payments": "merchant.com.ooosumfoods.thepepperedgoat"
      //       }
      //     }
      //   }
      // ],
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "thepepperedgoat",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": ""
      }
    }
  }
}